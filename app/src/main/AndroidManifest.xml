<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yashapps.mynotesapp">

    <application
        android:name=".NoteApp"
        android:allowBackup="true"
        android:icon="@mipmap/my_notes_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_notes_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyNotes">
        <activity
            android:name=".feature_note.presentation.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MyNotes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>